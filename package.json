		"@storybook/addon-vitest": "^9.1.17",
		"@storybook/sveltekit": "^9.1.17",
		"@sveltejs/adapter-auto": "^6.1.1",
		"@sveltejs/adapter-node": "^5.5.2",
		"@sveltejs/kit": "^2.50.2",
		"@sveltejs/vite-plugin-svelte": "^6.2.4",
		"@tailwindcss/forms": "^0.5.11",
		"@tailwindcss/typography": "^0.5.19",
		"@tailwindcss/vite": "^4.1.18",
		"@testing-library/jest-dom": "^6.9.1",
		"@testing-library/svelte": "^5.3.1",
		"@tsconfig/svelte": "^5.0.6",
		"@types/node": "^24.10.10",
		"@typescript-eslint/eslint-plugin": "^8.54.0",
		"@typescript-eslint/parser": "^8.54.0",
		"@vitest/browser": "^3.2.4",
		"@vitest/coverage-v8": "3.2.4",
		"axe-core": "^4.11.1",
		"cypress": "^15.9.0",
		"eslint": "^9.39.2",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-sonarjs": "^3.0.6",
		"eslint-plugin-storybook": "^9.1.17",
		"eslint-plugin-svelte": "^3.14.0",
		"globals": "^16.5.0",
		"jsdom": "^21.1.2",
		"lighthouse": "^12.8.2",
		"mdsvex": "^0.12.6",
		"mock-socket": "^9.3.1",
		"playwright": "^1.58.1",
		"prettier": "^3.8.1",
		"prettier-plugin-svelte": "^3.4.1",
		"prettier-plugin-tailwindcss": "^0.6.14",
		"rollup-plugin-visualizer": "^5.14.0",
		"storybook": "^9.1.17",
		"svelte": "^5.49.1",
		"svelte-check": "^4.3.6",
		"svelte-eslint-parser": "^1.4.1",
		"svelte-preprocess": "^6.0.3",
		"tailwindcss": "^4.1.18",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.54.0",
		"vite": "^7.3.1",
		"vite-plugin-devtools-json": "^1.0.0",
		"vite-plugin-pwa": "^1.2.0",
		"vitest": "^3.2.4",
		"vitest-browser-svelte": "^1.1.0"
	},
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"dev:token": "./scripts/dev-with-token.sh",
		"build": "vite build",
		"build:analyze": "ANALYZE=1 vite build --mode production && echo \"Visualizer generated at dist/stats.html\"",
		"analyze:parse": "bun run node ./scripts/parse-stats.js || node ./scripts/parse-stats.js",
		"analyze:lighthouse": "bun run ./scripts/run-lighthouse.bun.js",
		"analyze:report": "bun run ./scripts/analyze-lighthouse.bun.js",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test:unit": "vitest --project server",
		"test:unit:node": "vitest --project server --run",
		"test:client": "vitest --project client",
		"test:client:run": "vitest --project client --run",
		"test:single-node": "vitest run src/lib/stores/__tests__/leaderboardPreviewStore.test.ts --environment node",
		"codegen:leaderboard": "bunx quicktype -s schema -l ts -o src/lib/types/generated/leaderboard.ts src/features/leaderboard/schemas/leaderboard.snapshot.v1.json src/features/leaderboard/schemas/leaderboard.patch.v1.json",
		"test:ui": "vitest --project server --environment jsdom",
		"test:a11y": "vitest --run --environment jsdom src/lib/a11y/**/*.test.{js,ts}",
		"test:all": "bun run test:unit:node && bun run test:a11y",
		"coverage": "vitest --run --coverage",
		"coverage:client": "vitest --project client --run --coverage",
		"coverage:server": "vitest --project server --run --coverage",
		"test:browser": "vitest \"src/routes/**/*.spec.{ts,js}\"",
		"test": "bun run test:unit -- --run",
		"test:e2e:integration": "CYPRESS_USE_MOCK=false cypress run",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build"
	},
	"type": "module",
	"dependencies": {
		"@auth/sveltekit": "^1.11.1",
		"@opentelemetry/api": "^1.9.0",
		"@opentelemetry/core": "^2.5.0",
		"@opentelemetry/exporter-trace-otlp-http": "^0.211.0",
		"@opentelemetry/resources": "^2.5.0",
		"@opentelemetry/sdk-trace-web": "^2.5.0",
		"@opentelemetry/semantic-conventions": "^1.39.0",
		"add-to-calendar-button": "^2.13.5",
		"clsx": "^2.1.1",
		"nats.ws": "^1.30.3",
		"tailwind-merge": "^3.4.0"
	}
}
